\newcommand{\hr}{\centerline{\rule{\columnwidth}{1pt}}}
\newcommand{\thinhr}{\centerline{\rule{\columnwidth}{0.2pt}}}
% Adjust gap to affect spacing, page count
%\newcommand{\sect}[1]{\hr\par\vspace*{2pt}\textbf{#1}\par}
\newcommand{\sect}[2][]{\hr\par\vspace*{0.7pt}\textbf{#2}\quad\textbf{\secttitle@shaded@color@nu #1}\par\nopagebreak}
%\newcommand{\sect}[1]{\hr\par\vspace*{0.1pt}\textbf{#1}}

\newcommand{\subsect}[1]{{\warn #1}: }

% Mandatory indentation on subsidiary lines
\newcommand{\skipin}{\hspace*{12pt}}





\newcommand{\EX}[1]{\Ex{\sagecommand{#1}}}
\newcommand{\Ex}[1]{{\ex #1}}

\newcommand{\EXtt}[1]{\Ex{\sagecommandtt{#1}}}

\newcommand{\EXVAR}[1]{\Exvar{\sagevar{#1}}}
\newcommand{\EXvar}[1]{\Exvar{$#1$}}
\newcommand{\Exvar}[1]{{\exvar #1}}



\newcommand{\CAUTION}[1]{\Caution #1}
\newcommand{\Caution}{{\warn Caution}: }

\newcommand{\NOTE}[1]{\Note{#1}}
\newcommand{\Note}{{\warn Note}: }


\newcommand{\ex}{\ex@color@nu}
\newcommand{\exvar}{\exvar@color@nu}
\newcommand{\warn}{\bf\warn@color@nu}
\newcommand{\keystroke}{\keystroke@color@nu}

\newcommand{\KEYSTROKE}[1]{{\keystroke$\langle${#1}$\rangle$}}


\DeclareUrlCommand\sagecommand{\urlstyle{tt}}
\DeclareUrlCommand\sagevar{\def\UrlFont{\it}}
\newcommand{\sagecommandtt}[1]{\texttt{#1}}




\newcommand{\tabkey}{\KEYSTROKE{tab}}
\newcommand{\shiftEnterkey}{\KEYSTROKE{shift-enter}}
\newcommand{\altEnterkey}{\KEYSTROKE{alt-enter}}
\newcommand{\controlBackspacekey}{\KEYSTROKE{control-backspace}}




\newcommand{\BreakLineAndFulshLeft}{\\$\phantom{z}$\ \hfill}
\newcommand{\BreakLineAndIndent}[1][\skipin]{\\{}#1{}}
\newcommand{\BreakLineAndDoubleIndent}{\BreakLineAndIndent[\skipin\skipin]}



